[[1, 'H'], [1, 'H'], [2, 'H'], [2, 'D'], [3, 'H'], [3, 'A'], [4, 'D'], [4, 'H'], [5, 'D'], [5, 'D'], [6, 'D'], [6, 'A'], [7, 'A'], [7, 'H'], [8, 'A'], [8, 'D'], [9, 'A'], [9, 'A']]

